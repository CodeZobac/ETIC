
funcao calcularMedia(array):
    se tamanho(array) == 0:
        retorne "Array vazio"

    soma = 0
    cada valor em array:
        soma = soma + valor

    media = soma / tamanho(array)
    retorne media

array = [10, 20, 30, 40, 50]

imprime("A média dos valores no array é: {calcularMedia(array)}")
