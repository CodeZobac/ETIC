modelo Calculadora:
    funcao calcular(salario):
        se salario <= 1100:
            salario = salario - (salario * 0.11)
            print("O salario final é", salario)
        senao:
            salario = salario - (salario * 0.275)
            print("O salário final é", salario)

entrada("Qual o seu salário", salario)

Calculadora().calcular(salario)
