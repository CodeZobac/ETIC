
funcao ordenarArray(array):
    size = tamanho(array)
    cada i de (0, size - 1):
        para j de (0, size - i - 1):
            se array[j] < array[j + 1]:
                temp = array[j]
                array[j] = array[j + 1]
                array[j + 1] = temp
    retorne array


meuArray = [64, 34, 25, 12, 22, 11, 90]
arrayOrdenado = ordenarArray(meuArray)
imprime("Array ordenado por ordem decrescente: {arrayOrdenado}")
