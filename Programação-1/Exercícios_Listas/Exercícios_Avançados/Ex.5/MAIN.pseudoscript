
funcao rotacionarParaDireita(array):
    se array.tamanho() == 0:
        retorne "Array vazio"

    ultimoElemento = array[array.tamanho() - 1]
    cada i de array.tamanho() - 2 at√© 0 passo -1:
        array[i + 1] = array[i]
    array[0] = ultimoElemento

    retorne array

meuArray = [1, 2, 3, 4, 5]
arrayRotacionado = rotacionarParaDireita(meuArray)
imprime("Array rotacionado: {arrayRotacionado}")

