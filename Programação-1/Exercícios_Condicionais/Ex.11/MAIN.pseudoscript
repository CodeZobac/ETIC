modelo IMC:
    funcao calculo(peso, altura):
        imc = peso / (altura**2)
        se imc < 22:
            imprime("Abaixo do peso")
        senao imc >= 22 e imc < 27:
            imprime("Peso normal")
        senao imc >= 27:
            imprime("Acima do peso")

entrada("Qual o seu peso?", peso)
entrada("Qual a sua altura?", altura)

imprime("Resultado IMC: {calculo(peso, altura)}")
